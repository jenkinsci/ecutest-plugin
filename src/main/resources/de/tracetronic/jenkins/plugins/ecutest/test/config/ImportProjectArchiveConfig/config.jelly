<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="${%projectPath.title}" description="${%projectPath.description}" field="tmsPath">
        <f:textbox/>
    </f:entry>
    <f:entry title="${%importPath.title}" description="${%importPath.description}" field="importPath">
        <f:textbox/>
    </f:entry>
    <f:advanced>
        <f:entry title="${%importConfigPath.title}" description="${%importConfigPath.description}"
                 field="importConfigPath">
            <f:textbox/>
        </f:entry>
        <f:entry title="${%replaceFiles.title}" description="${%replaceFiles.description}" field="replaceFiles">
            <f:checkbox default="true"/>
        </f:entry>
    </f:advanced>
</j:jelly>
