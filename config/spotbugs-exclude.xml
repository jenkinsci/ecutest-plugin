<FindBugsFilter>
    <Match>
        <!-- Ignore warnings in Messages classes, generated by localizer. -->
        <Class name="~.*\.Messages"/>
    </Match>
    <Match>
        <!-- Ignore warnings in Lexer and Parser classes, generated by ANTLR. -->
        <Package name="~.*\.filter"/>
    </Match>
    <Match>
        <!-- Ignore warnings in test classes. -->
        <Class name="~.*\.*(Test|IT)$"/>
    </Match>
    <Match>
        <Bug pattern="FI_NULLIFY_SUPER"/>
    </Match>
    <Match>
        <Bug pattern="FI_MISSING_SUPER_CALL"/>
    </Match>
    <Match>
        <Bug pattern="FI_EMPTY"/>
    </Match>
</FindBugsFilter>
